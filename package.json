{
  "name": "badabam2019",
  "version": "0.0.1",
  "description": "Personal website of Jeremias Erbs",
  "main": "index.html",
  "scripts": {
    "start": "parcel ./src/index.html -d docs",
    "build": "npm run clear && parcel build ./src/index.html -d docs",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clear": "rm -rf ./docs ./cache"
  },
  "keywords": [],
  "author": "Jeremias Erbs <jerry@badabam.de> (http://www.badabam.de/)",
  "license": "UNLICENSED",
  "browserslist": [
    "last 2 version",
    "not dead",
    "not Explorer 11",
    "not ExplorerMobile 11"
  ],
  "_cache": {
    "inDev": true,
    "strategy": "default"
  },
  "cache": {
    "globPatterns": [
      "**/*.{html,js,css,jpg,png,woff,woff2}"
    ]
  },
  "devDependencies": {
    "autoprefixer": "9.5.1",
    "cssnano": "4.1.10",
    "parcel-bundler": "1.12.3",
    "parcel-plugin-static-files-copy": "2.0.0",
    "parcel-plugin-sw-cache": "0.2.7",
    "postcss-import": "12.0.1",
    "postcss-modules": "1.4.1",
    "postcss-nested": "4.1.2",
    "postcss-scss": "2.0.0",
    "postcss-short": "5.0.0",
    "postcss-sorting": "5.0.0",
    "posthtml-cache": "0.2.2",
    "posthtml-include": "1.2.0",
    "posthtml-md": "1.1.0",
    "posthtml-modules": "0.4.2",
    "sass": "1.20.1"
  }
}
